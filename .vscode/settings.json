{
    "eslint.autoFixOnSave": true,
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.wordWrap": "on",
    "files.eol": "\n",
    "javascript.validate.enable": false,
    "cSpell.words": [
        "Netlify",
        "devtools",
        "fiori",
        "middlewares",
        "northwind",
        "openui",
        "preload",
        "quartdark",
        "sourcemap",
        "thirdparty",
        "unfavorite"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    }
}
